{
  "version": 2,
  "routes": [
    { "src": "/.well-known/(.*)", "dest": "/.well-known/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://warpcast.com https://*.warpcast.com https://farcaster.xyz"
        }
      ]
    }
  ]
}
